  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "node server-dev.cjs",
    "dev:stripe": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "dev:full": "npm run build && wrangler pages dev dist --compatibility-date=2024-01-01",
    "build": "vite build",
    "build:prod": "node build-env.js && npx cross-env NODE_ENV=production npx vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "deploy:cloudflare": "npm run build:prod && wrangler pages deploy dist --project-name=aurion-saas",
    "performance:test": "lhci autorun --collect.url=https://0c114c34.aurion-saas.pages.dev",
    "performance:local": "npm run dev & sleep 3 && lhci autorun --collect.url=http://localhost:5173",
    "deploy:optimized": "powershell -File deploy-optimized.ps1",
    "typecheck": "tsc --noEmit"
  },
