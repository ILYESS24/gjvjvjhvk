{
  "name": "aurion-studio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:check": "tsc --noEmit && vite build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run lint && npm run test && npm run test:e2e",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "clean-db": "node clean-db.js",
    "clean-db:sql": "echo 'Execute clean-database.sql in your Supabase SQL Editor to remove all sample data'",
    "check": "npm run lint && npm run test && npm run build",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:preview": "npm run build && wrangler pages deploy dist --branch preview",
    "cf:login": "wrangler login",
    "cf:logout": "wrangler logout",
    "cf:whoami": "wrangler whoami"
  },
